  <img class='kidpic'src="https://static.pexels.com/photos/89873/pexels-photo-89873.jpeg" alt="kids"></img>
    <div class="teen-text">
      <a href="/advisorportfolioc/{{data.advisorClink}}" class="btn btn-primary">Teens</a>
    </div>
  </div>
</div>
{{/if}}
